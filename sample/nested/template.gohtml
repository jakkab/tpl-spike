<!doctype html>
<html>
<head>
    <meta charset="utf-8">
    <title>A simple, clean, and responsive HTML invoice template</title>
</head>

<body>
    <div>
        {{range .list}}
        <tr>
            <td>
                {{.listProperty}}
                {{range .nestedList}}
                    {{.}}
                {{end}}
            </td>
        </tr>
        {{end}}
    </div>
</body>
</html>
